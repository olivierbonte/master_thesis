   "outputs": [],
   "source": [
    "import dask.array as da\n",
    "\n",
    "x = da.random.random((10000,10000), chunks = \"16 MiB\")\n",
    "x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5d297aae-7a57-4234-a3e9-2c86622778c7",
   "metadata": {},
   "outputs": [],
   "source": [
    "y = (x + x.T)  - x.mean(axis = 0)\n",
    "y.sum().compute()"
   ]
  },
